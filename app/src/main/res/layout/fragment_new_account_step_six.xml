<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <data>

    </data>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".view.fragments.user_cycle.NewAccountStepSixFragment"
        android:orientation="vertical"
        android:background="@color/colorFragment"
        android:gravity="center_horizontal"
        >

        <ImageView
            android:id="@+id/new_account_step_six_img_condition"
            android:layout_marginStart="@dimen/_50sdp"
            android:layout_marginEnd="@dimen/_50sdp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
             android:src="@drawable/ic_iconcondition"
            android:layout_marginBottom="@dimen/_25sdp"

            />
        <ImageView
            android:id="@+id/new_account_step_six_img_condition_ok"
            android:layout_marginStart="@dimen/_50sdp"
            android:layout_marginEnd="@dimen/_50sdp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/ic_iconcondition_ok"
            android:visibility="gone"
            android:layout_marginBottom="@dimen/_25sdp"
            />


    </LinearLayout>
</layout>